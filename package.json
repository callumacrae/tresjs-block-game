{
  "name": "tresjs-block-game",
  "version": "0.0.0",
  "description": "A block game-like environment powered by TresJS",
  "author": "Callum Macrae <callum@macr.ae>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@tresjs/cientos": "^2.1.3",
    "@tresjs/core": "^2.1.2",
    "@types/three": "^0.152.1",
    "@vueuse/core": "^10.1.2",
    "alea": "^1.0.1",
    "pinia": "^2.1.3",
    "simplex-noise": "^4.0.1",
    "three": "^0.153.0",
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vite": "^4.3.5",
    "vue-tsc": "^1.6.4"
  },
  "resolutions": {
    "three": "^0.153.0"
  }
}
